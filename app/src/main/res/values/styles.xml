<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!--Style for custom Toast-->
    <style name="exampleToast">
        <item name="stColorBackground">#94ffab</item>
        <item name="stTextColor">#000</item>
        <item name="stSolidBackground">true</item>
        <item name="stIconStart">@drawable/ic_stars_black_24dp</item>
        <item name="stIconEnd">@drawable/ic_stars_black_24dp</item>
        <item name="strokeColor">#4CAF50</item>
        <item name="strokeWidth">3dp</item>
        <item name="cornerRadius">3dp</item>
    </style>

</resources>
